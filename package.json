{
  "name": "intensifyhq",
  "version": "1.0.0",
  "description": "Work Like You Train - Productivity app merging gym principles with work",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler publish",
    "migrate:local": "wrangler d1 migrations apply intensifyhq-db --local",
    "migrate:remote": "wrangler d1 migrations apply intensifyhq-db --remote",
    "db:init": "wrangler d1 execute intensifyhq-db --file=./migrations/0001_initial.sql",
    "tail": "wrangler tail",
    "test": "vitest",
    "format": "prettier --write ."
  },
  "keywords": [
    "productivity",
    "task-management",
    "cloudflare-workers",
    "d1-database",
    "stripe"
  ],
  "author": "IntensifyHQ",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "prettier": "^3.2.4",
    "vitest": "^1.2.1",
    "wrangler": "^3.24.0"
  },
  "dependencies": {
    "itty-router": "^4.0.25",
    "stripe": "^14.14.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
